<AutoCompleteTextView android:textSize="10sp"
 android:layout_width="90dip" android:id="@+id/search" android:layout_height="40dip" android:layout_y="2dip" android:layout_x="300dip"></AutoCompleteTextView>

<Button android:layout_x="386dip" android:layout_y="2dip"
		android:layout_width="wrap_content" android:layout_height="40dip" android:id="@+id/go" android:text="Go"></Button>
</AbsoluteLayout>
